[run]
source = .
omit =
    */tests/*
    */test_*.py
    */.venv/*
    */venv/*
    */site-packages/*
    */__pycache__/*
    */migrations/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
precision = 2

[html]
directory = htmlcov
